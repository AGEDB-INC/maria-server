SET @debug_key_flags=TRUE;
CREATE PROCEDURE debug_show_key_flags()
BEGIN
IF @debug_key_flags IS TRUE
THEN
FLUSH TABLES;
SET debug_dbug='+d,key';
SELECT * FROM t1 LIMIT 0;
SET debug_dbug='';
END IF;
END;
